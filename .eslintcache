<Route\n          path=\"/dashboard/:action\"\n          render={(rp) => <Form {...rp} getProjects={getProjects} />}\n        />\n        <ul className=\"projects\">\n          {state.projects.map((project) => (\n            <div className=\"project\" key={project.id}>\n              <h2>{project.title}</h2>\n              <h4>{project.description}</h4>\n              <Button variant=\"primary\" className=\"projectbuttons\"\n                onClick={() => {\n                  dispatch({ type: \"select\", payload: project });\n                  props.history.push(\"/dashboard/edit\");\n                }}\n              >\n                Edit Project\n              </Button>\n              <Button variant=\"outline-danger\" className=\"projectbuttons\"\n                onClick={() => {\n                  fetch(url + \"/projects/\" + project.id, {\n                    method: \"delete\",\n                    headers: {\n                      Authorization: \"bearer \" + token,\n                    },\n                  }).then(() => getProjects());\n                }}\n              >\n                Delete Project\n              </Button>\n            </div>\n          ))}\n        </ul>\n      </div>\n    );\n  };\n  return projects ? loaded() : <Spinner animation=\"border\" variant=\"success\" />;\n};\n\nexport default Dashboard;","/Users/owner/Desktop/webDev/generalAssembly/seir-flex-zen-work/projects/p4/home_maker_client/src/components/Form.js",[],"/Users/owner/Desktop/webDev/generalAssembly/seir-flex-zen-work/projects/p4/home_maker_client/src/components/Layout.js",[],"/Users/owner/Desktop/webDev/generalAssembly/seir-flex-zen-work/projects/p4/home_maker_client/src/components/Footer.js",["55"],"const Footer = () => {\n    return(<footer className=\"smalls\">\n        <a className=\"port\" href=\"https:carloberardelli.com\" target=\"_blank\">Carlo Berardelli 2020</a>\n    </footer>)\n}\n\nexport default Footer",{"ruleId":"56","replacedBy":"57"},{"ruleId":"58","rep